<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
	<bean id="missedCallService" class="com.dependencyinjectionexample.MissedCallService"></bean>
	<bean id="helloTuneService" class="com.dependencyinjectionexample.HelloTuneService"></bean>
	<bean id="voiceCallsService" class="com.dependencyinjectionexample.VoiceCallsService"></bean>
	
	<bean id="airtel" class="com.dependencyinjectionexample.Airtel">
		<property name="service" ref="voiceCallsService"></property>
	</bean>
	
	
	
</beans>