<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
	<bean id="mathCheat" class="com.springdependencyinjectionexample.MathCheat"></bean>
	<bean id="scienceCheat" class="com.springdependencyinjectionexample.ScienceCheat"></bean>
	
	<bean id="Student" class="com.springdependencyinjectionexample.Student">
		<!--   <property name="cheat" ref="mathCheat"></property> -->
		<constructor-arg name="cheat" ref="scienceCheat"></constructor-arg>
	</bean>
	
</beans>